#ifndef LINUX_SERVER_LIB_KV_STORE_GLOBAL_CONFIG_H_
#define LINUX_SERVER_LIB_KV_STORE_GLOBAL_CONFIG_H_

#define PROJECT_NAME "@PROJECT_NAME@"
#define PROJECT_VER  "@PROJECT_VERSION@"
#define PROJECT_VER_MAJOR "@PROJECT_VERSION_MAJOR@"
#define PROJECT_VER_MINOR "@PROJECT_VERSION_MINOR@"
#define PROJECT_VER_PATCH "@PROJECT_VERSION_PATCH@"
#define BUILD_SUPPORTED_REDIS_VERSION "@BUILD_SUPPORTED_REDIS_VERSION@"

#define CHECK_VERSION_GREATER_THEN_OR_EQUAL_BUILD_SUPPORTED_REDIS_VERSION(major, minor, patch) \
    VERSION_CHECK(@BUILD_SUPPORTED_REDIS_VERSION_MAJOR@, @BUILD_SUPPORTED_REDIS_VERSION_MINOR@, @BUILD_SUPPORTED_REDIS_VERSION_PATCH@) >= VERSION_CHECK(major, minor, patch)
#define CHECK_VERSION_GREATER_THEN_BUILD_SUPPORTED_REDIS_VERSION(major, minor, patch) \
    VERSION_CHECK(@BUILD_SUPPORTED_REDIS_VERSION_MAJOR@, @BUILD_SUPPORTED_REDIS_VERSION_MINOR@, @BUILD_SUPPORTED_REDIS_VERSION_PATCH@) > VERSION_CHECK(major, minor, patch)
#define CHECK_VERSION_LESS_THEN_OR_EQUAL_BUILD_SUPPORTED_REDIS_VERSION(major, minor, patch) \
    VERSION_CHECK(@BUILD_SUPPORTED_REDIS_VERSION_MAJOR@, @BUILD_SUPPORTED_REDIS_VERSION_MINOR@, @BUILD_SUPPORTED_REDIS_VERSION_PATCH@) <= VERSION_CHECK(major, minor, patch)
#define CHECK_VERSION_LESS_THEN_BUILD_SUPPORTED_REDIS_VERSION(major, minor, patch) \
    VERSION_CHECK(@BUILD_SUPPORTED_REDIS_VERSION_MAJOR@, @BUILD_SUPPORTED_REDIS_VERSION_MINOR@, @BUILD_SUPPORTED_REDIS_VERSION_PATCH@) < VERSION_CHECK(major, minor, patch)

#define VERSION_CHECK(major, minor, patch) ((major << 16) | (minor << 8) | (patch))

#endif //LINUX_SERVER_LIB_KV_STORE_GLOBAL_CONFIG_H_
